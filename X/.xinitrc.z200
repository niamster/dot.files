xrdb ~/.Xdefaults
sudo sh -c 'echo -n conservative > /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor; echo -n conservative > /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor'
sudo sh -c 'echo -n enabled > /sys/class/thermal/thermal_zone0/mode'
tint2 &
feh --bg-scale ~/images/wallpaper.* &
urxvtd -q -o -f
xxkb &
sudo nm-applet --sm-disable &
parcellite &

exec openbox --config-file ~/.config/openbox/openbox.xml
