xrdb ~/.Xdefaults
tint2 &
feh --bg-scale ~/images/wallpaper.* &
urxvtd -q -f
xxkb &
parcellite &

sudo sh -c 'echo -n conservative > /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor; echo -n conservative > /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor'
sudo sh -c 'echo -n enabled > /sys/class/thermal/thermal_zone0/mode'

exec openbox --config-file ~/.config/openbox/openbox.xml
